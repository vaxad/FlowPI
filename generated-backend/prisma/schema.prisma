generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("BACKEND_DATABASE_URL")
}

model test1 {
 id  String  @id @default(auto()) @map("_id") @db.ObjectId
  name String
  pass BigInt
}

model test3 {
 id  String  @id @default(auto()) @map("_id") @db.ObjectId
  email String
  password BigInt
}

